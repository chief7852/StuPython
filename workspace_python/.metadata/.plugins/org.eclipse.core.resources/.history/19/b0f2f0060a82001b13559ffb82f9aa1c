package day02;

import java.awt.EventQueue;

import javax.swing.JFrame;
import javax.swing.JLabel;

public class MyFrame07 {

	private JFrame frame;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					MyFrame07 window = new MyFrame07();
					window.frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the application.
	 */
	public MyFrame07() {
		initialize();
	}

	/**
	 * Initialize the contents of the frame.
	 */
	private void initialize() {
		frame = new JFrame();
		frame.setBounds(100, 100, 450, 300);
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.getContentPane().setLayout(null);
		
		JLabel lblCom = new JLabel("New label");
		lblCom.setBounds(12, 31, 57, 22);
		frame.getContentPane().add(lblCom);
		
		JLabel lblMine = new JLabel("New label");
		lblMine.setBounds(12, 82, 57, 22);
		frame.getContentPane().add(lblMine);
	}

}
