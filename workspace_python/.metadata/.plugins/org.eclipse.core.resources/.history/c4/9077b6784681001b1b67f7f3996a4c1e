class Dog :
    def __init__(self):
        self.flag_bark = True
        
    def cutSungdae(self):
        self.flag_bark = False
    
class Bird:
    def __init__(self):
        self.idx_fly = 0
    def training(self):
        self.idx_fly += 1
    def training_hard(self,power):
        self.idx_fly += power
        
class GaeSae(Dog,Bird):
    def __init__(self):
        pass
    
