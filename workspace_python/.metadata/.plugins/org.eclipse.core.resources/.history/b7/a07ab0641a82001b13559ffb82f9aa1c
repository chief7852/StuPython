import sys
from PyQt5.QtWidgets import QApplication, QMainWindow
from PyQt5 import uic 

form_class = uic.loadUiType("Hello.ui")[0]
class Myqt01(QMainWindow, form_class):
    def __init__(self):
        super().__init__()
        self.setupUi(self)

    def onClick(self):
        self.l.setText("Good Evening")
if __name__ == "__main__":
    app = QApplication(sys.argv)
    demoWindow = Myqt01()
    demoWindow.show() 
    app.exec_()