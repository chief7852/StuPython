import random
from networkx.generators.tests.test_small import null

com = ""
mine = ""
while mine !=:
    mine = input("홀/짝을 선택하시오...")

rnd = random.random()
if rnd >0.5 :
    com = "홀"
else:
    com = "짝"

if com == mine :
    result = "승리"
else:
    result = "패배"

if mine != null :
    print("컴 : ",com)
    print("나 : ",mine)
    print("결과 : ",result)